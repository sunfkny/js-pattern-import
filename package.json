{
  "name": "js-pattern-import",
  "displayName": "js-pattern-import",
  "description": "A VS Code extension for batch importing files in a folder with pattern matching and automatic generation of import/export statements. Perfect for batch importing image assets.",
  "publisher": "sunfkny",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunfkny/js-pattern-import"
  },
  "engines": {
    "vscode": "^1.105.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Js Pattern Import",
      "properties": {
        "js.pattern.import.pattern": {
          "type": "string",
          "default": "*.{png,jpg,jpeg,svg}",
          "description": "Pattern for import"
        }
      }
    },
    "commands": [
      {
        "command": "js-pattern-import.main",
        "title": "Js Pattern Import"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == javascript",
          "command": "js-pattern-import.main",
          "group": "9_cutcopypaste"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/mocha": "^10.0.0",
    "@types/node": "~24.10.2",
    "@types/vscode": "^1.105.1",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "glob": "^10.3.10",
    "eslint": "^9.39.1",
    "mocha": "^10.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}
